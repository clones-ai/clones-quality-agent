{
  "name": "pipeline",
  "version": "2.0.12",
  "main": "process.js",
  "scripts": {
    "test": "bun test",
    "test:grading": "bun run scripts/test-grading.ts",
    "test:grading:unit": "bun run scripts/test-grading.ts --unit",
    "test:grading:integration": "bun run scripts/test-grading.ts --integration",
    "test:grading:all": "bun run scripts/test-grading.ts --all",
    "start": "bun run src/index.ts",
    "stats": "bun run scripts/stats.ts",
    "download": "bun run scripts/download.ts",
    "build:windows": "bun build --compile --target=bun-windows-x64 ./src/index.ts --outfile target/pipeline-win-x64.exe",
    "build:macos": "bun build --compile --target=bun-macos-arm64 ./src/index.ts --outfile target/pipeline-macos-arm64",
    "build:linux": "bun build --compile --target=bun-linux-x64 ./src/index.ts --outfile target/pipeline-linux-x64",
    "build": "bun build:windows && bun build:macos && bun build:linux"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "canvas": "^3.1.2",
    "cli-table3": "^0.6.5",
    "mongodb": "^6.18.0",
    "ndjson": "^2.0.0",
    "openai": "^5.12.2",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1",
    "tiktoken": "^1.0.22",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.2"
  }
}